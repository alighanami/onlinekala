<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <system.webServer>
    <handlers>
      <add name="Python FastCGI"
           path="*"
           verb="*"
           modules="FastCgiModule"
           scriptProcessor="C:\djangoapp\venv\Scripts\python.exe|C:\djangoapp\venv\Lib\site-packages\wfastcgi.py"
           resourceType="Unspecified"
           requireAccess="Script" />
    </handlers>
  </system.webServer>

  <appSettings>
    <add key="WSGI_HANDLER" value="onlinekala.wsgi.application" />
    <add key="PYTHONPATH" value="C:\djangoapp" />
  </appSettings>

</configuration>
